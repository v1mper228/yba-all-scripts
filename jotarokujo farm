game:GetService("Workspace").Living["psyeoh80_YBA"].Humanoid.HipHeight = 30
game:GetService("Workspace").Living["psyeoh80_YBA"].HumanoidRootPart.CFrame = game:GetService("Workspace").Living["Jotaro Kujo"].HumanoidRootPart.CFrame
function kill()
game:GetService("Workspace").Living.psyeoh80_YBA.HumanoidRootPart.StandAttach.WorldCFrame = game:GetService("Workspace").Living["Jotaro Kujo"].UpperTorso.CFrame
end
function checkstand()
if game:GetService("Workspace").Living["psyeoh80_YBA"].SummonedStand.Value then
	print("debil stend tyt")
else
local args = {
    [1] = "ToggleStand",
    [2] = "Toggle"
}

game:GetService("Players").LocalPlayer.Character.RemoteFunction:InvokeServer(unpack(args))
    end
end
function barrage()
-- Script generated by SimpleSpy - credits to exx#9394

local jl = {
    [1] = "InputBegan",
    [2] = {
        ["Input"] = Enum.KeyCode.R
    }
}

game:GetService("Players").LocalPlayer.Character.RemoteEvent:FireServer(unpack(jl))
-- Script generated by SimpleSpy - credits to exx#9394
wait(0.2)
local mm = {
    [1] = "InputEnded",
    [2] = {
        ["Input"] = Enum.KeyCode.R
    }
}

game:GetService("Players").LocalPlayer.Character.RemoteEvent:FireServer(unpack(mm))
-- Script generated by SimpleSpy - credits to exx#9394

local kk = {
    [1] = "InputBegan",
    [2] = {
        ["Input"] = Enum.KeyCode.E
    }
}

game:GetService("Players").LocalPlayer.Character.RemoteEvent:FireServer(unpack(kk))
-- Script generated by SimpleSpy - credits to exx#9394
wait(0.2)
local oi = {
    [1] = "InputEnded",
    [2] = {
        ["Input"] = Enum.KeyCode.E
    }
}

game:GetService("Players").LocalPlayer.Character.RemoteEvent:FireServer(unpack(oi))
-- Script generated by SimpleSpy - credits to exx#9394

local dick = {
    [1] = "InputBegan",
    [2] = {
        ["Input"] = Enum.KeyCode.T
    }
}

